   "source": [
    " def numberOfSteps(self, num: int) -> int:\n",
    "        steps = 0\n",
    "\n",
    "        while num != 0:\n",
    "            steps +=1\n",
    "            if num & 1:\n",
    "                num -=1\n",
    "            else:\n",
    "                num >>=1\n",
    "        return steps"
   ]
  }
